{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "target": "es2016",
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": [
      "esnext", "dom"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
  }
}